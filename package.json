    "babel-eslint": "^10.0.3",
    "devtron": "^1.4.0",
    "electron": "^11.5.0",
    "electron-builder": "^22.8.0",
    "electron-debug": "^3.1.0",
    "electron-devtools-installer": "^3.1.1",
    "electron-packager": "^15.0.0",
    "eslint": "^6.3.0",
    "eslint-loader": "^3.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.92.0"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.6.0"
