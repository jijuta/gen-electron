{
  "name": "saesolgen",
  "version": "0.7.2",
  "description": "Saesolsoft GenApp",
  "productName": "Saesolsoft GenApp",
  "cordovaId": "com.saesol.sasolgen",
  "author": "dokjin@gmail.com",
  "private": true,
  "scripts": {
    "copy:win": "copy .\\app.js .\\node_modules\\@quasar\\app\\templates\\entry\\app.js",
    "copy:mac": "cp ./app.js ./node_modules/@quasar/app/templates/entry/app.js",
    "lint": "eslint --ext .js,.vue src",
    "test": "echo \"See package.json => scripts for available tests.\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build -m electron",
    "build:pwa": "quasar build -m pwa",
    "start": "node --openssl-legacy-provider node_modules/@quasar/app/bin/quasar dev -m electron",
    "test:unit": "jest --updateSnapshot",
    "test:unit:coverage": "jest --coverage",
    "test:unit:watch": "jest --watch",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\""
  },
  "dependencies": {
    "@quasar/extras": "^1.9.0",
    "@vue/babel-preset-app": "^4.4.6",
    "ag-grid-community": "^21.2.0",
    "ag-grid-enterprise": "^21.2.0",
    "ag-grid-vue": "^21.2.0",
    "axios": "^0.19.0",
    "core-js": "^2.5.7",
    "dynamic-file-require": "0.0.9",
    "electron-updater": "^4.3.1",
    "fast-levenshtein": "^2.0.6",
    "jquery": "^3.5.1",
    "knex": "^0.19.5",
    "moment": "^2.24.0",
    "monaco-editor-vue": "^1.0.10",
    "mssql": "^5.1.0",
    "mysql": "^2.17.1",
    "mysql2": "^3.14.4",
    "pg": "^7.12.1",
    "pretty-bytes": "^5.3.0",
    "quasar": "^1.12.13",
    "readline": "^1.3.0",
    "save-dev": "0.0.1-security",
    "shelljs": "^0.8.4",
    "string-similarity": "^4.0.1",
    "vue": "2.7.14",
    "vue-diagrams": "0.0.1-development",
    "vue-i18n": "^8.14.0",
    "vue-monaco": "^1.2.1",
    "vue-property-decorator": "^9.0.0",
